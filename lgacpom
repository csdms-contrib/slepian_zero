#!/bin/tcsh
#
# An in-repository local version of the alias gacpom with meaningful commit messages

git add guyotphysics.m
git commit -m "A simple earthquake seismogram plotting routine"
git push

git add PDF/*
git commit -m "Images generated by some of the eponymous codes in this repository"
git push 

git add tinitalia.m
git commit -m "Converts TINITALY topography into *.mat and *hdr files"
git push origin master

git add puzzle.m
git commit -m "Identifies overlapping rims in a tile set"
git push origin master

git add mosaic.m
git commit -m "Makes a composite image from partially overlapping tiles"
git push origin master

git add rimcheck.m
git commit -m "Checks overlapping rims in a tile set"
git push origin master

git add tinitaly.m
git commit -m "Matches TINITALY topography to RAPIDEYE tiles"
git push origin master

git add tinitalh.m
git commit -m "Reads TINITALY topography (headers only)"
git push origin master

git add README.md
git commit -m "Y'all heard me: Read me!"
git push origin master

git add tinitalg.m
git commit -m "Reads TINITALY topography (grid and data)"
git push origin master

git add rinitaly.m
git commit -m "Reads Italian river network database"
git push origin master

git add rapideye.m
git commit -m "Reads RAPIDEYE imagery"
git push origin master

git add rapideyg.m
git commit -m "Returns RAPIDEYE grid"
git push origin master

git add pic2prof.m
git commit -m "Takes picture, makes a profile"
git push origin master

git add lgacpom
git commit -m "An instruction set for meaningful commit messages"
git push origin master

git add readGEBCO.m
git commit -m "Reads GEBCO bathymetry files and reformats them for storage as MAT files"
git push

git add nwis.m
git commit -m "Reads water (streamflow etc) data from the USGS, through the RDB format"
git push

git add randij.m
git commit -m "Returns a random size for a matrix of a certain total number of elements"
git push

git add gebco.m
git commit -m "Returns GEBCO bathymetry interpolated to the requested location"
git push

git add blockmean.m
git commit -m "Calculates means of submatrices within a matrix"
git push

git add croputm.m
git commit -m "Limit view and transform WGS84 geocentric degrees to UTM coordinates"
git push

git add utmlabels.m
git commit -m "Plots good-looking UTM labels on the current axes"
git push

git add utm2utm.m
git commit -m "Converts coordinates in one UTM zone to those in another"
git push

git add point2line.m
git commit -m "Distance of a SINGLE POINT to a line between a PAIR of points in the plane"
git push

git add point3line.m
git commit -m "Distance of a SINGLE POINT to a line between a PAIR of points in three-dimensional space"
git push

git add pointdist.m
git commit -m "Distances of POINTS to a PARAMETERIZED LINE in the plane"
git push

git add point2points.m
git commit -m "Distances of POINTS to a LEAST-SQUARES LINE through ANOTHER set of points in the plane"
git push

git add pathfinderpoints.m
git commit -m "Reads a generic POINTS coordinate CSV file from GPS Pathfinder Office"
git push

git add pathfinderlines.m
git commit -m "Reads a generic LINES coordinate CSV file from GPS Pathfinder Office"
git push

git add linedist.m
git commit -m "Distance of a line to a line"
git push

git add lineshift.m
git commit -m "Shifts a set of scattered points along their best-fitting line"
git push






